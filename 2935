using System;
using System.Numerics;

public class Program {
    public static void Main() {
            BigInteger input01 = BigInteger.Parse(Console.ReadLine());                
            string operation = Console.ReadLine();
            BigInteger input02 = BigInteger.Parse(Console.ReadLine());

            if (operation == "+") Console.WriteLine(input01 + input02);
            else if (operation == "*") Console.WriteLine(input01 * input02);
    }
}
